					"    const accessToken = pm.environment.get('access')",
					"",
					"    if (basicToken && basicToken !== 'null') {",
					"        pm.request.headers.add({",
					"            key: 'Authorization',",
					"            value: `Token ${basicToken}`",
					"        })",
					"    } else if (accessToken && accessToken !== 'null') {",
					"        pm.request.headers.add({",
					"            key: 'Authorization',",
					"            value: `Token ${accessToken}`",
					"        }) ",
					"    }",
					"",
					"    pm.request.headers.add({",
					"        key: 'Content-Type',",
					"        value: 'application/json'",
					"    })",
