{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/undefined/**/*.json\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@fontsource/montserrat": "^4.5.5",
    "@fontsource/open-sans": "^4.5.5",
    "@fontsource/roboto": "^4.5.3",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@vue/devtools-api": "^6.0.2",
    "axios": "^0.25.0",
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.17.3",
    "core-js": "^3.6.5",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.0",
    "lodash": "^4.17.21",
    "mutationobserver-shim": "^0.3.7",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "vue": "^2.6.11",
    "vue-axios": "^3.4.1",
    "vue-country-flag": "^2.3.2",
    "vue-i18n": "^8.26.3",
    "vue-router": "^3.2.0",
    "vue-sessionstorage": "^1.0.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@mdi/font": "^6.5.95",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-vuex": "^4.5.15",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-bootstrap-vue": "^0.8.2",
    "vue-cli-plugin-fontawesome": "^0.3.1",
    "vue-cli-plugin-i18n": "^2.3.1",
    "vue-cli-plugin-vuetify": "^2.4.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/attribute-hyphenation": "error",
      "vue/html-end-tags": "error",
      "vue/html-indent": "warn",
      "vue/attributes-order": "warn",
      "vue/order-in-components": "warn",
      "vue/this-in-template": "error",
      "vue/component-tags-order": [
        "error",
        {
          "order": [
            [
              "template",
              "script"
            ],
            "style"
          ]
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
