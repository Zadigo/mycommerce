{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "src/*": [
                "src/*"
            ],
            "components/*": [
                "src/components/*"
            ],
            "layouts/*": [
                "src/layouts/*"
            ],
            "pages/*": [
                "src/pages/*"
            ],
            "assets/*": [
                "src/assets/*"
            ],
            "stores/*": [
                "src/stores/*"
            ],
            "data/*": [
                "src/data/*"
            ],
            "composables/*": [
                "src/composables/*"
            ],
            "vue$": [
                "node_modules/vue/dist/vue.runtime.esm-bundler.js"
            ]
        }
    },
    "exclude": [
        "dist",
        "node_modules"
    ]
}
